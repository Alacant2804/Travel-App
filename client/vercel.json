{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/auth/(.*)",
      "destination": "https://travel-app-backend-delta.vercel.app/api/auth"
    },
    {
      "source": "/api/(.*)",
      "destination": "https://travel-app-backend-delta.vercel.app/api"
    }
  ]
}
